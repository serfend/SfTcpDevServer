<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>serfend</Authors>
    <Company>sf</Company>
    <Description>server for report log</Description>
    <PackageProjectUrl>github.com/serfend/devserver</PackageProjectUrl>

    <PackageId>SfTcpDevServer</PackageId>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="SfCommonUtils" Version="1.0.0" />
  </ItemGroup>

</Project>
